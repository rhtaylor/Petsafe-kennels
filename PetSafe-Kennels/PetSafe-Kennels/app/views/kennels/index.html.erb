<p id="notice"><%= notice %></p>

<h1>Kennels</h1>

<table>
  <thead>
    <tr>
      <th>Kennel type</th>
      <th>Materials</th>
      <th>Cost</th>
      <th>Labor</th>
      <th>Charge</th>
      <th>Date</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kennels.each do |kennel| %>
      <tr>
        <td><%= kennel.kennel_type %></td>
        <td><%= kennel.materials %></td>
        <td><%= kennel.cost %></td>
        <td><%= kennel.labor %></td>
        <td><%= kennel.charge %></td>
        <td><%= kennel.date %></td>
        <td><%= kennel.customer_id %></td>
        <td><%= link_to 'Show', kennel %></td>
        <td><%= link_to 'Edit', edit_kennel_path(kennel) %></td>
        <td><%= link_to 'Destroy', kennel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kennel', new_kennel_path %>
